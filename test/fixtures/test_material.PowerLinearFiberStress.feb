<?xml version='1.0' encoding='UTF-8'?>
<febio_spec version="2.5">
  <Module type="solid"/>
  <Material>
    <material type="solid mixture" id="1">
      <solid type="fiber-pow-linear">
        <!-- Make this fiber family slack; test λn < 1, in x. -->
        <E>52</E>
        <beta>2.5</beta>
        <lam0>1.07</lam0>
        <fiber type="angles">
          <theta>0</theta>
          <phi>90</phi>
        </fiber>
      </solid>
      <solid type="fiber-pow-linear">
        <!-- Put this fiber family in toe region; test 1 < λn < λ0, in y. -->
        <E>52</E>
        <beta>2.5</beta>
        <lam0>1.07</lam0>
        <fiber type="angles">
          <theta>90</theta>
          <phi>90</phi>
        </fiber>
      </solid>
      <solid type="fiber-pow-linear">
        <!-- Put this fiber family in linear region; test λ0 < λn, in z -->
        <E>52</E>
        <beta>2.5</beta>
        <lam0>1.07</lam0>
        <fiber type="angles">
          <theta>0</theta>
          <phi>0</phi>
        </fiber>
      </solid>
      <!-- <solid type="Holmes-Mow"> -->
      <!--   <E>0.5</E> -->
      <!--   <v>0</v> -->
      <!--   <beta>3.4</beta> -->
      <!-- </solid> -->
    </material>
  </Material>
  <Geometry>
    <Nodes>
      <node id="1">0,0,0</node>
      <node id="2">1,0,0</node>
      <node id="3">1,1,0</node>
      <node id="4">0,1,0</node>
      <node id="5">0,0,1</node>
      <node id="6">1,0,1</node>
      <node id="7">1,1,1</node>
      <node id="8">0,1,1</node>
    </Nodes>
    <Elements type="hex8" mat="1">
      <elem id="1">1, 2, 3, 4, 5, 6, 7, 8</elem>
    </Elements>
    <NodeSet name="−x_face">
      <node id="1"/>
      <node id="4"/>
      <node id="5"/>
      <node id="8"/>
    </NodeSet>
    <NodeSet name="+x_face">
      <node id="2"/>
      <node id="3"/>
      <node id="6"/>
      <node id="7"/>
    </NodeSet>
    <NodeSet name="−y_face">
      <node id="1"/>
      <node id="2"/>
      <node id="5"/>
      <node id="6"/>
    </NodeSet>
    <NodeSet name="+y_face">
      <node id="3"/>
      <node id="4"/>
      <node id="7"/>
      <node id="8"/>
    </NodeSet>
    <NodeSet name="−z_face">
      <node id="1"/>
      <node id="2"/>
      <node id="3"/>
      <node id="4"/>
    </NodeSet>
    <NodeSet name="+z_face">
      <node id="5"/>
      <node id="6"/>
      <node id="7"/>
      <node id="8"/>
    </NodeSet>
  </Geometry>
  <Boundary>
    <prescribe bc="x" node_set="+x_face">
      <scale lc="2">-0.05</scale>
    </prescribe>
    <prescribe bc="y" node_set="+y_face">
      <scale lc="2">0.05</scale>
    </prescribe>
    <prescribe bc="z" node_set="+z_face">
      <scale lc="2">0.12</scale>
    </prescribe>
    <fix bc="x" node_set="−x_face"/>
    <fix bc="y" node_set="−y_face"/>
    <fix bc="z" node_set="−z_face"/>
  </Boundary>
  <LoadData>
    <loadcurve extend="constant" id="1" type="linear">
      <point>0.0, 1.0</point>
      <point>1.0, 1</point>
    </loadcurve>
    <loadcurve extend="extrapolate" id="2" type="linear">
      <point>0, 0</point>
      <point>1, 1</point>
    </loadcurve>
  </LoadData>
  <Step name="Step1">
    <Control>
      <time_steps>1</time_steps>
      <step_size>1.0</step_size>
      <dtol>0.001</dtol>
      <etol>0.01</etol>
      <rtol>0</rtol>
      <lstol>0.9</lstol>
      <min_residual>1e-20</min_residual>
      <qnmethod>0</qnmethod>
      <reform_each_time_step>True</reform_each_time_step>
      <diverge_reform>True</diverge_reform>
      <max_refs>15</max_refs>
      <max_ups>10</max_ups>
      <plot_level>PLOT_MUST_POINTS</plot_level>
      <time_stepper>
        <dtmin>0.1</dtmin>
        <max_retries>5</max_retries>
        <opt_iter>10</opt_iter>
        <dtmax lc="1">1</dtmax>
      </time_stepper>
    </Control>
  </Step>
</febio_spec>
